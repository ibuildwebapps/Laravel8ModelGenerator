{
    "name": "ibuildwebapps/laravel-schema-generator",
    "description": "Generate Laravel models, migrations, and form requests from MySQL database schema",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Paul",
            "email": "paul@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/console": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.0|^3.0"
    },
    "autoload": {
        "psr-4": {
            "IBuildWebApps\\SchemaGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IBuildWebApps\\SchemaGenerator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IBuildWebApps\\SchemaGenerator\\SchemaGeneratorServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "pest"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
